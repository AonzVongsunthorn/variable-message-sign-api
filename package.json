{
  "name": "variable-message-sign-api",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"migrations/**/*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "env TZ=Etc/Universal nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate:create": "node --require ts-node/register ./node_modules/typeorm/cli.js -d migrations/ormconfig.ts migration:create -n",
    "migrate:up": "node --require ts-node/register ./node_modules/typeorm/cli.js -d migrations/ormconfig.ts migration:run",
    "migrate:down": "node --require ts-node/register ./node_modules/typeorm/cli.js -d migrations/ormconfig.ts migration:revert",
    "migrate:refresh": "npm run schema:drop && npm run migrate:up",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js -n migrations/seeder-ormconfig.ts config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js -n migrations/seeder-ormconfig.ts seed",
    "seed:refresh": "npm run migrate:refresh && npm run seed:run",
    "release": "release-it"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/crypto-js": "^4.1.2",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "express-session": "^1.17.3",
    "faker": "^6.6.6",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.0",
    "node-cmd": "^5.0.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.13",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-seeding": "^1.6.1",
    "webpack": "^5.81.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
